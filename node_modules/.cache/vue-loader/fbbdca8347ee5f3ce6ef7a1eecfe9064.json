\r\n\t\t\t\t\t\t<span>{{ form.TakeMedicineNumber }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-col>\r\n\t\t\t\t<el-col :span=\"8\">\r\n\t\t\t\t\t<el-form-item label=\"实际服药次数\">\r\n\t\t\t\t\t\t<span>{{ form.ActualTakeMedicineNumber }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-col>\r\n\r\n\t\t\t\t<el-col :span=\"8\">\r\n\t\t\t\t\t<el-form-item label=\"服药率\">\r\n\t\t\t\t\t\t<span>{{ form.MedicationRate+'%' }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-col>\r\n\r\n\r\n\t\t\t\t<!-- <el-col :span=\"8\">\r\n          <el-form-item label=\"是否电话随访\">\r\n            <span v-if=\"form.IsPhone\">{{ form.IsPhone==true?'是':'否'}}</span>\r\n          </el-form-item>\r\n        </el-col> -->\r\n\r\n\t\t\t\t<!-- <el-col :span=\"8\">\r\n          <el-form-item label=\"是否就诊\">\r\n            <span v-if=\"form.IsDiagnosis\">{{ form.IsDiagnosis==true?'是':'否'}}</span>\r\n          </el-form-item>\r\n        </el-col> -->\r\n\r\n\t\t\t\t<!-- <el-col :span=\"8\">\r\n          <el-form-item label=\"是否确认结果\">\r\n            <span  v-if=\"form.IsResult\">{{ form.IsResult==true?'是':'否'}}</span>\r\n          </el-form-item>\r\n        </el-col> -->\r\n\r\n\r\n\r\n\t\t\t\t<el-col :span=\"8\">\r\n\t\t\t\t\t<el-form-item label=\"治疗方案\">\r\n\t\t\t\t\t\t<span>{{ form.TreatmentPlanName }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-col>\r\n\t\t\t\t<el-col :span=\"8\">\r\n\t\t\t\t\t<el-form-item label=\"是否规范\">\r\n\t\t\t\t\t\t<span>{{ form.IsStandard?'是':'否' }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-col>\r\n\r\n\r\n\t\t\t\t<el-col :span=\"8\">\r\n\t\t\t\t\t<el-form-item label=\"备注\">\r\n\t\t\t\t\t\t<span>{{ form.Remarks }}</span>\r\n\t\t\t\t\t</el-form-item>\r\n\t\t\t\t</el-col>\r\n\r\n\r\n\r\n\r\n\t\t\t</el-row>\r\n\t\t\t-\r\n\t\t</el-form>\r\n\t\t<div slot=\"footer\">\r\n\t\t\t<el-button class=\"cancelbtn\" @click=\"close\">取 消</el-button>\r\n\t\t</div>\r\n\t</el-dialog>\r\n</template>\r\n<script>\r\n\timport {\r\n\t\tplysettlementList,\r\n\t} from '@/api/PulmonaryClose.js'\r\n\texport default {\r\n\t\tname: \"Edit\",\r\n\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\ttitle: \"\",\r\n\t\t\t\tcloseId: '',\r\n\t\t\t\tinnerVisible: false,\r\n\t\t\t\tflag: \"\",\r\n\t\t\t\tform: {},\r\n\r\n\r\n\t\t\t};\r\n\t\t},\r\n\t\tcreated() {\r\n\r\n\r\n\t\t},\r\n\r\n\r\n\t\tmethods: {\r\n\t\t\t// 确认新增？修改\r\n\t\t\tasync show(flag, id) {\r\n\t\t\t\tthis.innerVisible = true;\r\n\t\t\t\tthis.title = \"详情\";\r\n\t\t\t\tthis.flag = flag;\r\n\t\t\t\tif (flag == 'details') {\r\n\t\t\t\t\tconst res = await plysettlementList({\r\n\t\t\t\t\t\tcloseId: id\r\n\t\t\t\t\t})\r\n\t\t\t\t\tlet {\r\n\t\t\t\t\t\tStatusCode,\r\n\t\t\t\t\t\tMessage\r\n\t\t\t\t\t} = res\r\n\t\t\t\t\tif (StatusCode !== 200) {\r\n\t\t\t\t\t\treturn this.$baseMessage(Message || '数据获取失败', 'error')\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthis.form = {\r\n\t\t\t\t\t\t...res.Response\r\n\t\t\t\t\t}\r\n\t\t\t\t} else if (flag == 'details2') {\r\n\r\n\t\t\t\t\tconst res = await plysettlementList({\r\n\t\t\t\t\t\tdiagnosisId: id\r\n\t\t\t\t\t})\r\n\t\t\t\t\tlet {\r\n\t\t\t\t\t\tStatusCode,\r\n\t\t\t\t\t\tMessage\r\n\t\t\t\t\t} = res\r\n\t\t\t\t\tif (StatusCode !== 200) {\r\n\t\t\t\t\t\treturn this.$baseMessage(Message || '数据获取失败', 'error')\r\n\t\t\t\t\t}\r\n\t\t\t\t\tthis.form = {\r\n\t\t\t\t\t\t...res.Response\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t},\r\n\r\n\t\t\t//取消\r\n\t\t\tclose() {\r\n\t\t\t\tthis.innerVisible = false;\r\n\t\t\t\t// 重置数据(封装的方法)\r\n\t\t\t\tthis.form = this.$options.data().form;\r\n\t\t\t\tthis.$refs[\"form\"].resetFields();\r\n\t\t\t},\r\n\t\t},\r\n\t};\r\n</script>\r\n\r\n<style lang=scss scoped>\r\n\t::v-deep .el-form-item__label {\r\n\t\tfont-size: 16px;\r\n\t\tcolor: #999999 100%;\r\n\r\n\t}\r\n\r\n\tspan {\r\n\t\tfont-size: 16px;\r\n\t\tcolor: #252D57;\r\n\r\n\t}\r\n</style>\r\n"]}]}