le\" class=\"dialog-title\">\r\n      <div class=\"sectionstly\"></div>\r\n      <span class=\"title-text\">{{ title }}</span>\r\n      <div class=\"button-right\">\r\n        <span class=\"title-close\" @click=\"close\"></span>\r\n      </div>\r\n    </div>\r\n    <el-form class=\"form_row\" ref=\"form\" :model=\"form\" label-width=\"100px\">\r\n      <div class=\"detailstitlo\">\r\n        <p class=\"xqtitolstyle\">基本信息</p>\r\n      </div>\r\n      <!-- <el-row :gutter=\"20\">\r\n        <el-col :span=\"7\">\r\n          <el-form-item label=\"行政区划:\" prop=\"RegionCode\">\r\n              <MyCascader v-model=\"form.RegionCode\" :props=\"props\" clearable></MyCascader>\r\n            </el-form-item>\r\n        </el-col>\r\n      </el-row> -->\r\n\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item label=\"姓名\">\r\n            <span>{{ form.Name }}</span>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"8\">\r\n          <el-form-item label=\"性别\">\r\n            <span>{{ form.SexName }}</span>\r\n          </el-form-item>\r\n        </el-col>\r\n\r\n        <el-col :span=\"8\">\r\n          <el-form-item label=\"年龄\">\r\n            <span>{{ form.Age }}</span>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <el-form-item label=\"联系方式\">\r\n            <span>{{ form.Phone }}</span>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item label=\"出生日期\">\r\n            <span>{{ form.BirthDay | parseTime('{y}-{m}-{d}') }}</span>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"8\">\r\n          <el-form-item label=\"证件号\">\r\n            <span>{{ form.CardNo }}</span>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <el-row :gutter=\"20\">\r\n        <el-col :span=\"8\">\r\n          <e