FA,EAwFAwB,OAxFA,EAwFA;MAAA;;MAAA;QAAA;;QAAA;UAAA;YAAA;cAAA;gBAAA,MAEAH,iBAFA;kBAAA;kBAAA;gBAAA;;gBAGAC,oBAHA,GAGAtB,yBAHA;gBAAA;gBAAA,OAIAX;kBAAAiC;gBAAA,WAJA;;cAAA;gBAIAP,GAJA;gBAMAC,UANA,GAMAD,GANA,CAMAC,UANA,EAMAC,OANA,GAMAF,GANA,CAMAE,OANA;;gBAAA,MAOAD,iBAPA;kBAAA;kBAAA;gBAAA;;gBAAA,kCAQA,oBACAC,iBADA,EACA,OADA,CARA;;cAAA;gBAYA;;gBACA;;gBACA,sCAdA,CAeA;;;gBAfA;gBAAA;;cAAA;gBAAA,MAgBAI,iBAhBA;kBAAA;kBAAA;gBAAA;;gBAiBAC,qBAjBA,GAiBAtB,yBAjBA;gBAkBA6B,aAlBA,GAkBA7B,kBAlBA;gBAmBA8B,iBAnBA,GAmBA9B,sBAnBA,EAoBA;;gBApBA;gBAAA,OAqBAZ;kBACA2C,2CADA;kBACAC,4BADA;kBAEAC;gBAFA,EArBA;;cAAA;gBAqBAlB,KArBA;gBA0BAC,YA1BA,GA0BAD,KA1BA,CA0BAC,UA1BA,EA0BAC,SA1BA,GA0BAF,KA1BA,CA0BAE,OA1BA;;gBAAA,MA2BAD,mBA3BA;kBAAA;kBAAA;gBAAA;;gBAAA,kCA4BA,oBACAC,mBADA,EACA,OADA,CA5BA;;cAAA;gBAgCA;;gBACA;;gBACA;;cAlCA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAoCA;EA5HA;AA/BA","names":["InspectApplicationlist","InspectApplicationecharts","InspectApplicationeModify","InspectApplicationedeletes","InspectApplicationresult","InspectApplicationeModify2","InspectApplicationmake","components","MedicinerateEchart_3","StatisticsEcharts","CheckaPpointmentEdit","Checktab5Edit","Checktab6Edit","props","data","radio1","form","copyData","tableData","Echartdata","created","watch","inputData","handler","val","deep","immediate","methods","funInspectApplicationlist","ApplicationItemCategory","SourceId","res","StatusCode","Message","funInspectApplicationecharts","diagnosisId","handlerEdit","flag","inspectApplicationId","checkaPpointmentEdit","hasinfo","personId","sourceId","PersonId","checktab5Edit","inspectResult","inspectResultTime","InspectApplicationId","InspectResult","InspectResultTime"],"sourceRoot":"src/views/patientManagement/patientManagement/components/tabComponents","sources":["checktab5.vue"],"sourcesContent":["<template>\r\n  <div class=\"room\">\r\n    <!-- 图表 -->\r\n    <div class=\"heardechart\">\r\n      <p>痰菌检查病原学阳性率</p>\r\n      <MedicinerateEchart_3 :EchartList=\"Echartdata\"></MedicinerateEchart_3>\r\n    </div>\r\n    <div class=\"heardechart2\">\r\n      <StatisticsEcharts :EchartList=\"Echartdata\"></StatisticsEcharts>\r\n    </div>\r\n    <el-button :disabled=\"inputData.IsClosed ? true : false\" type=\"primary\" round @click=\"handlerEdit('Phlegm')\">检查预约</el-button>\r\n    <!-- 表格 -->\r\n    <div class=\"tablebox\">\r\n      <vxe-table\r\n        auto-resize\r\n        border\r\n        align=\"center\"\r\n        show-header-overflow\r\n        show-overflow\r\n        :row-config=\"{ isHover: true }\"\r\n        height=\"300\"\r\n        style=\"overflow: auto\"\r\n        :data=\"tableData\"\r\n      >\r\n        <vxe-column field=\"RegisterTime\" title=\"确诊时间\">\r\n          <template slot-scope=\"{ row }\">\r\n            <span>\r\n              {{ row.RegisterTime | parseTime('{y}-{m}-{d}') }}\r\n            </span>\r\n          </template>\r\n        </vxe-column>\r\n        <vxe-column field=\"ResponsibilityName\" title=\"责任医生\"></vxe-column>\r\n        <vxe-column field=\"AdminOrganizationName\" title=\"管理机构\"></vxe-column>\r\n        <vxe-column field=\"ApplicationTypeName\" title=\"检查原因\"></vxe-column>\r\n        <vxe-column field=\"ApplicationInspectTime\" title=\"预约日期\">\r\n          <template slot-scope=\"{ row }\">\r\n            <span>\r\n              {{ row.ApplicationInspectTime | parseTime('{y}-{m}-{d}') }}\r\n            </span>\r\n          </template>\r\n        </vxe-column>\r\n        <vxe-column field=\"InspectResultName\" title=\"检查结果\"></vxe-column>\r\n        <vxe-column field=\"InspectResultTime\" title=\"检查日期\">\r\n          <template slot-scope=\"{ row }\">\r\n            <span>\r\n              {{ row.InspectResultTime | parseTime('{y}-{m}-{d}') }}\r\n            </span>\r\n          </template>\r\n        </vxe-column>\r\n        <vxe-column title=\"操作\" width=\"300\" fixed=\"right\">\r\n          <template #default=\"{ row }\">\r\n            <div class=\"flex_center\">\r\n              <el-link type=\"warning\" :disabled=\"row.InspectResultTime == null\" @click=\"handlerEdit('Modify1', row)\">详情</el-link>\r\n              <div class=\"Divider\"></div>\r\n              <el-link type=\"warning\" :disabled=\"inputData.IsClosed ? true : false\" v-if=\"!row.InspectResultName\" @click=\"handlerEdit('Modify', row)\">修改预约</el-link>\r\n              <div class=\"Divider\" v-if=\"!row.InspectResultName\"></div>\r\n              <el-link type=\"success\" :disabled=\"inputData.IsClosed ? true : false\" v-if=\"row.InspectResultTime == null\" @click=\"handlerEdit('warning', row)\">结果录入</el-link>\r\n              <el-link type=\"warning\" :disabled=\"inputData.IsClosed ? true : false\" @click=\"handlerEdit('Modify2', row)\" v-else>修改录入</el-link>\r\n              <div class=\"Divider\"></div>\r\n              <el-link type=\"danger\" @click=\"handlerEdit('delete', row)\">删除</el-link>\r\n            </div>\r\n          </template>\r\n        </vxe-column>\r\n      </vxe-table>\r\n    </div>\r\n    <!--预约弹窗 -->\r\n    <CheckaPpointmentEdit ref=\"checkaPpointmentEdit\" @checkaPpointmentEdit=\"checkaPpointmentEdit\"></CheckaPpointmentEdit>\r\n    <!-- 增删改弹窗 -->\r\n    <Checktab5Edit ref=\"checktab5Edit\" @submit=\"funInspectApplicationlist\"></Checktab5Edit>\r\n    <Checktab6Edit ref=\"checktab6Edit\" @checktab5Edit=\"checktab5Edit\"></Checktab6Edit>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MedicinerateEchart_3 from './echeatComponents/medicinerateEchart_3.vue'\r\nimport StatisticsEcharts from './echeatComponents/statisticsEcharts.vue'\r\nimport CheckaPpointmentEdit from './echeatComponents/checkaPpointmentEdit.vue'\r\nimport Checktab5Edit from \"./EditComponents/checktab5Edit.vue\";\r\nimport Checktab6Edit from \"./EditComponents/checktab6Edit.vue\";\r\nimport {\r\n  InspectApplicationlist,\r\n  InspectApplicationecharts,\r\n  InspectApplicationeModify,\r\n  InspectApplicationedeletes,\r\n  InspectApplicationresult,\r\n  InspectApplicationeModify2,\r\n  InspectApplicationmake\r\n} from '@/api/InspectApplication.js'\r\nexport default {\r\n  components: { MedicinerateEchart_3, StatisticsEcharts, CheckaPpointmentEdit, Checktab5Edit, Checktab6Edit }\r\n  ,\r\n  props: ['inputData'],\r\n  data () {\r\n    return {\r\n      radio1: '全部',\r\n      form: {},\r\n      copyData: [{}],\r\n      tableData: [],\r\n      Echartdata: {}\r\n\r\n    }\r\n  },\r\n  async created () {\r\n    this.copyData[0] = this.inputData;\r\n    await this.funInspectApplicationlist()\r\n\r\n  },\r\n  watch: {\r\n    inputData: {\r\n      handler (val) {\r\n        if (val.DiagnosisId) {\r\n          this.copyData[0] = this.inputData;\r\n          this.funInspectApplicationecharts()\r\n        }\r\n      },\r\n      deep: true,\r\n      immediate: true\r\n    }\r\n  },\r\n  methods: {\r\n    // 列表\r\n    async funInspectApplicationlist () {\r\n      const res = await InspectApplicationlist({ ApplicationItemCategory: 2, SourceId: this.inputData.DiagnosisId })\r\n      let { StatusCode, Message } = res\r\n      if (!res || StatusCode !== 200) {\r\n        return this.$baseMessage(Message || '数据获取失败', 'error')\r\n      }\r\n      this.tableData = res.Response.items;\r\n      await this.funInspectApplicationecharts()\r\n    },\r\n    // 图表\r\n    async funInspectApplicationecharts () {\r\n      let diagnosisId = this.copyData[0].DiagnosisId || this.copyData[0].SourceId\r\n      const res = await InspectApplicationecharts(diagnosisId, 2)\r\n      let { StatusCode, Message } = res\r\n\r\n      if (!res || StatusCode !== 200) {\r\n        return this.$baseMessage(Message || '数据获取失败', 'error')\r\n      }\r\n      this.Echartdata = res.Response\r\n    },\r\n    handlerEdit (flag, data) {\r\n      // 预约\r\n      if (flag == 'Phlegm') {\r\n        this.$refs.checkaPpointmentEdit.show(flag, this.inputData)\r\n      } else if (flag == 'Modify1') {\r\n        // 修改预约\r\n        this.$refs.checktab6Edit.show(flag, data, this.inputData)\r\n      } else if (flag == 'Modify') {\r\n        // 修改预约\r\n        this.$refs.checkaPpointmentEdit.show(flag, data, this.inputData)\r\n      } else if (flag == 'Modify2') {\r\n        this.$refs.checktab5Edit.show(flag, data, this.inputData, 'A0015')\r\n      } else if (flag == 'warning') {\r\n        // 结果录入\r\n        this.$refs.checktab5Edit.show(flag, data, this.inputData, 'A0015')\r\n        this.funInspectApplicationecharts()\r\n        this.funInspectApplicationecharts()\r\n      } else if (flag == 'delete') {\r\n        // 删除\r\n        this.$baseConfirm(\"确定要删除所选项吗？\", '删除', async () => {\r\n          const inspectApplicationId = data.InspectApplicationId\r\n          const res = await InspectApplicationedeletes([inspectApplicationId])\r\n          let { StatusCode, Message } = res;\r\n          if (StatusCode != 200) {\r\n            return this.$baseMessage(\r\n              Message || '删除失败', 'error'\r\n            )\r\n          }\r\n          this.$baseMessage('删除成功', \"success\")\r\n          this.funInspectApplicationlist()\r\n          this.funInspectApplicationecharts()\r\n        },null,\"error\",)\r\n      }\r\n    },\r\n    // 检查预约\r\n    async checkaPpointmentEdit (flag, form, hasinfo) {\r\n      // 新增预约\r\n      if (flag == 'Phlegm') {\r\n        let personId = this.copyData[0].PersonId\r\n        let sourceId = this.copyData[0].DiagnosisId || this.copyData[0].SourceId\r\n        const res = await InspectApplicationmake({ ...form, PersonId: personId, SourceId: sourceId })\r\n        let { StatusCode, Message } = res;\r\n        if (StatusCode != 200) {\r\n          return this.$baseMessage(\r\n            Message || '预约失败', 'error'\r\n          )\r\n        }\r\n        this.$baseMessage('预约成功', \"success\")\r\n        this.funInspectApplicationlist()\r\n        this.funInspectApplicationecharts()\r\n      } else if (flag == 'Modify') {\r\n        // 预约修改\r\n        const inspectApplicationId = form.InspectApplicationId\r\n        const res = await InspectApplicationeModify({ inspectApplicationId }, hasinfo)\r\n        let { StatusCode, Message } = res;\r\n        if (StatusCode != 200) {\r\n          return this.$baseMessage(\r\n            Message || '修改失败', 'error'\r\n          )\r\n        }\r\n        this.$baseMessage('修改成功', \"success\")\r\n        this.funInspectApplicationlist()\r\n        this.funInspectApplicationecharts()\r\n      }\r\n    },\r\n\r\n    async checktab5Edit (flag, form, hasinfo) {\r\n      //录入结果修改\r\n      if (flag == 'Modify2') {\r\n        const inspectApplicationId = form.InspectApplicationId\r\n        const res = await InspectApplicationeModify2({ inspectApplicationId }, hasinfo)\r\n\r\n        let { StatusCode, Message } = res;\r\n        if (StatusCode != 200) {\r\n          return this.$baseMessage(\r\n            Message || '修改失败', 'error'\r\n          )\r\n        }\r\n        this.$baseMessage('修改成功', \"success\")\r\n        this.funInspectApplicationlist()\r\n        this.funInspectApplicationecharts()\r\n        // 录入结果\r\n      } else if (flag == 'warning') {\r\n        let inspectApplicationId = form.InspectApplicationId\r\n        let inspectResult = form.InspectResult\r\n        let inspectResultTime = form.InspectResultTime\r\n        // let  fileRelationGuid=form.InspectApplicationId\r\n        const res = await InspectApplicationresult({\r\n          InspectApplicationId: inspectApplicationId, InspectResult: inspectResult,\r\n          InspectResultTime: inspectResultTime,\r\n        })\r\n\r\n        let { StatusCode, Message } = res;\r\n        if (StatusCode != 200) {\r\n          return this.$baseMessage(\r\n            Message || '录入失败', 'error'\r\n          )\r\n        }\r\n        this.$baseMessage('录入成功', \"success\")\r\n        this.funInspectApplicationlist()\r\n        this.funInspectApplicationecharts()\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.room2 {\r\n  width: 100%;\r\n  height: 500px;\r\n  position: relative;\r\n}\r\n\r\n.heardechart {\r\n  width: 50%;\r\n  height: 300px;\r\n  float: left;\r\n}\r\n\r\n.heardechart2 {\r\n  width: 50%;\r\n  height: 350px;\r\n  float: left;\r\n}\r\n\r\n.heardechart > p {\r\n  color: black;\r\n  font-size: 25px;\r\n  font-weight: 600;\r\n  margin: 15px 0 0 20px;\r\n}\r\n\r\n.tablebox {\r\n  width: 100%;\r\n  height: 100%;\r\n  float: left;\r\n  margin-top: 10px;\r\n}\r\n\r\n.appointment {\r\n  width: 100%;\r\n  height: 40px;\r\n  background: green;\r\n  margin-top: 30px;\r\n}\r\n\r\n.el-button--medium.is-round {\r\n  // position: absolute;\r\n  // right: 0;\r\n  margin-top: 30px;\r\n  margin-left: 90%;\r\n}\r\n</style>"]}]}