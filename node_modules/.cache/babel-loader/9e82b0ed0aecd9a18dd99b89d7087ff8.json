 xAxis: [\r\n          {\r\n            type: 'category',\r\n            // this.numberData.xAxis\r\n            data:this.numberData.xAxis,\r\n            axisTick: {\r\n              alignWithLabel: true\r\n            }\r\n          }\r\n        ],\r\n        yAxis: [\r\n          {\r\n            type: 'value'\r\n          }\r\n        ],\r\n        series: [\r\n\r\n          {\r\n            name: '发病的数量',\r\n            type: 'bar',\r\n\r\n            color: ['#0D5DE2'],\r\n            // 设置柱状图柱子的宽度\r\n            barWidth: '30%',\r\n            // this.numberData.CasesNumberyAxis\r\n            data: this.numberData.CasesNumberyAxis\r\n          }\r\n        ]\r\n      })\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.initechart()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.room {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n#map {\r\n  width: 100%;\r\n  height: 80%;\r\n}\r\n\r\n.title {\r\n\r\n  margin: 25px 0 0 22px;\r\n  display: flex;\r\n}\r\n\r\n.title>img {\r\n  margin-right: 13px;\r\n}\r\n\r\n.title>span {\r\n  font-weight: 400;\r\n  color: #2A2A2A;\r\n  margin-top: 0;\r\n\r\n}\r\n</style>"]}]}