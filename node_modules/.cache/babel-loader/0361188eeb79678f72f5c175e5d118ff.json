cAGAC,4BAHA;cAIAC;gBACAC,YADA;gBAEAR;cAFA;YAJA;UADA,CATA;UAqBAS,WArBA;UAsBAjB,YAtBA;UAuBAD,MAvBA;UAwBAmB,YAxBA;UAyBAC;YACAR;cACAjB,UADA;cAEAsB,cAFA;cAGAI;YAHA;UADA,CAzBA;UAgCAC;YACA3B;UADA,CAhCA;UAmCAb,OACA;YAAAyC;UAAA,CADA,EAEA;YAAAA;UAAA,CAFA;QAnCA,CADA;MAnBA;IAiEA;EAxEA,CAzBA;EAqGAC,OArGA,qBAqGA;IACA;EACA;AAvGA","names":["props","data","charts","num","watch","EchartList","handler","computed","nums","methods","echartsValue","initechart","tooltip","trigger","show","grid","width","height","legend","top","right","orient","itemWidth","itemHeight","selectedMode","series","hoverAnimation","type","color","radius","avoidLabelOverlap","label","normal","position","formatter","textStyle","fontSize","left","bottom","emphasis","fontWeight","labelLine","value","mounted"],"sourceRoot":"src/views/patientManagement/patientManagement/components/tabComponents/echeatComponents","sources":["medicineEchart.vue"],"sourcesContent":["<template>\r\n  <div class=\"echartroom\">\r\n    <div ref=\"charts\" id=\"map\"></div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport * as echarts from \"echarts\";\r\nexport default {\r\n  props: ['EchartList'],\r\n  data () {\r\n    return {\r\n      charts: null,\r\n      num: 0\r\n    }\r\n  },\r\n  watch: {\r\n    EchartList: {\r\n      handler: function (newval, oldval) {\r\n        this.$nextTick(() => {\r\n          // props不能直接复制\r\n          this.num = newval\r\n          this.initechart()\r\n        })\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    nums () {\r\n      return this.num.AdverseReactionCount + this.num.DiscontinueTreatmentCount + this.num.AbnormalInspectionCount\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    echartsValue () {\r\n      return this.num.AbnormalMedicationCount\r\n    },\r\n    initechart () {\r\n      this.charts = echarts.init(this.$refs.charts);\r\n\r\n      this.charts.setOption({\r\n        tooltip: {\r\n          trigger: 'item',\r\n          show: false,// 关闭鼠标移上去后显示浮框信息\r\n        },\r\n        grid: {\r\n          width: '100%',\r\n          height: '100%'\r\n        },\r\n\r\n        legend: {\r\n          top: '36%',\r\n          right: '10%',\r\n          orient: 'vertical',\r\n          itemWidth: 20,\r\n          itemHeight: 20,\r\n          selectedMode: false, // 取消标题点击后事件-这里显示和隐藏指定项\r\n\r\n        },\r\n        series: [\r\n          {\r\n            hoverAnimation: false, // 取消掉环形图鼠标移上去时自动放大\r\n            // name: '异常服药次数',\r\n            type: 'pie',\r\n            color: ['#1E6BEC', '#F0F0F0'],\r\n            // 控制饼状图的环形宽度\r\n            radius: ['58%', '70%'],\r\n            avoidLabelOverlap: false,\r\n            // 设置数据中间样式????\r\n            label: {\r\n              normal: {\r\n                show: true,\r\n                position: 'center',\r\n                formatter: this.echartsValue,\r\n                textStyle: {\r\n                  fontSize: 25,\r\n                  color: '#252D57'\r\n                }\r\n              },\r\n\r\n            },\r\n            left: '20%',\r\n            right: '20%',\r\n            top: 0,\r\n            bottom: '0%',\r\n            emphasis: {\r\n              label: {\r\n                show: true,\r\n                fontSize: '40',\r\n                fontWeight: 'bold'\r\n              }\r\n            },\r\n            labelLine: {\r\n              show: false\r\n            },\r\n            data: [\r\n              { value: this.num.AbnormalMedicationCount, },\r\n              { value: this.nums },\r\n\r\n\r\n            ]\r\n          }\r\n        ]\r\n\r\n      })\r\n    },\r\n  },\r\n\r\n\r\n  mounted () {\r\n    this.initechart()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.echartroom {\r\n  width: 100%;\r\n  height: 83%;\r\n  position: relative;\r\n}\r\n\r\n#map {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n</style>"]}]}