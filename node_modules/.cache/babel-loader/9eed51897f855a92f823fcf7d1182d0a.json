A;QAEAC,0DAFA;QAGAC;MAHA,EA3BA;MA+BA;MACAC,gBAhCA;MAiCAC,QAjCA;MAkCAC;QACAC,aADA;QAEAC;MAFA,CAlCA;MAsCAC,aAtCA;MAuCAC,kBAvCA,CAuCA;;IAvCA;EA0CA,CApDA;EAsDAC,OAtDA,qBAsDA;IAAA;;IAAA;MAAA;QAAA;UAAA;YAAA;cAAA;cAAA,OACA,4BADA;;YAAA;YAAA;cAAA;UAAA;QAAA;MAAA;IAAA;EAGA,CAzDA;EA0DAC,OA1DA,qBA0DA;IACA;EACA,CA5DA;EA6DAC;IACA;IACAC,oBAFA,kCAEA;MAAA;;MAAA;QAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAAA,OACA1C,kDACA,eADA,GAEA,WAFA,EADA;;cAAA;gBACA2C,GADA;gBAOAC,UAPA,GAUAD,GAVA,CAOAC,UAPA,EAQAC,OARA,GAUAF,GAVA,CAQAE,OARA,EASAC,QATA,GAUAH,GAVA,CASAG,QATA;;gBAAA,MAWA,8BAXA;kBAAA;kBAAA;gBAAA;;gBAAA,kCAYA,iDAZA;;cAAA;gBAcA;gBACA;;cAfA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAgBA,CAlBA;IAmBA;IACAC,mBApBA,iCAoBA;MAAA;;MAAA;QAAA;QAAA;UAAA;YAAA;cAAA;gBACA;gBADA;gBAAA,OAEA5C,6CAFA;;cAAA;gBAEAwC,GAFA;gBAGA;;cAHA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAIA,CAxBA;IAyBArB,QAzBA,oBAyBA0B,IAzBA,EAyBAC,OAzBA,EAyBA;MAAA;QAAA;QAAA;UAAA;YAAA;cAAA;gBAAA;gBAAA,OACA/C;kBACAgD,8BADA;kBAEAf,aAFA;kBAGAC;gBAHA,EADA;;cAAA;gBACAO,GADA;gBAMA/B,IANA,GAMA,EANA;;gBAOA;kBACAA;gBACA;;gBACAqC;;cAVA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAWA,CApCA;IAqCA;IACAE,eAtCA,6BAsCA;MAAA;;MACA;QACA,4CADA,CACA;;QACA,8DAFA,CAEA;;QACA;MACA,CAJA;IAKA,CA5CA;IA6CA;IACAC,WA9CA,uBA8CAC,IA9CA,EA8CAzC,IA9CA,EA8CA;MAAA;;MACA;QACA,yCADA,CAEA;MACA,CAHA,MAGA;QACA;MACA,CAFA,MAEA;QACA;MACA,CAFA,MAEA;QACA;QACA;UAAA;UAAA;YAAA;cAAA;gBAAA;kBACA;kBACA0C,OAFA,GAEA1C,YAFA;kBAAA;kBAAA,OAGAX,qCAHA;;gBAAA;kBAGA0C,GAHA;kBAKAC,UALA,GAQAD,GARA,CAKAC,UALA,EAMAC,OANA,GAQAF,GARA,CAMAE,OANA,EAOAC,QAPA,GAQAH,GARA,CAOAG,QAPA;;kBASA;;kBACA;;gBAVA;gBAAA;kBAAA;cAAA;YAAA;UAAA;QAAA,KAWA,IAXA,EAWA,OAXA;MAYA,CAdA,MAcA;QACA;QACA;MACA;IACA,CAxEA;IAyEA;IACAS,gBA1EA,4BA0EAtC,KA1EA,EA0EA;MACA;IACA,CA5EA;IA8EAuC,cA9EA,0BA8EAH,IA9EA,EA8EA9B,IA9EA,EA8EAkC,OA9EA,EA8EA;MAAA;;MAAA;QAAA;QAAA;UAAA;YAAA;cAAA;gBACAH,OADA,GACA/B,YADA;gBAAA;gBAAA,OAGAmC;kBACAC;gBADA,GAGAF,OAHA,CAHA;;cAAA;gBAGAd,GAHA;gBASAC,UATA,GAWAD,GAXA,CASAC,UATA,EAUAC,OAVA,GAWAF,GAXA,CAUAE,OAVA;;gBAAA,MAYAD,iBAZA;kBAAA;kBAAA;gBAAA;;gBAAA,kCAaA,+CAbA;;cAAA;gBAeA;;cAfA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAgBA,CA9FA;IAgGA;IACAgB,WAjGA,yBAiGA;MACA;IACA,CAnGA;IAoGAC,UApGA,wBAoGA;MAAA;;MAAA;QAAA;QAAA;UAAA;YAAA;cAAA;gBACA;gBACA;gBACA;gBACA;gBACA;gBACA;gBACA;gBACA;gBACA;gBACA;gBAVA;gBAAA,OAWA7D,kDACA,eADA,GAEA,WAFA,EAXA;;cAAA;gBAWA2C,GAXA;gBAeA;gBACA;;gBACA;;cAjBA;cAAA;gBAAA;YAAA;UAAA;QAAA;MAAA;IAkBA,CAtHA;IAwHA;IACAmB,iBAzHA,6BAyHAC,OAzHA,EAyHA;MACA;MACA;IACA,CA5HA;IA6HA;IACAC,qBA9HA,iCA8HAC,QA9HA,EA8HA;MACA;MACA;IACA;EAjIA;AA7DA","names":["toapplysettlement","deletetuberculosisdiagnosiss","getOrganizationList","getorgcascader","getuserDrop","name","components","Edit","RecurrenceEdit","MyCascader","CaseEdit","NewEdit","data","props","emitPath","label","children","value","multiple","checkStrictly","lazy","propslist","lazyLoad","form","SelectOnly","AdminOrganization","IsClosed","radio1","tenantData","OrganizationId","OrganizationName","Children","showSearch","total","paParams","PageNumber","PageSize","tableData","tableHeight","created","mounted","methods","funtoapplysettlement","res","StatusCode","Message","Response","getorganizationlist","node","resolve","Org_OrganizationId","initTableHeight","handlerEdit","flag","CloseId","changeSubmitTime","recurrenceEdit","hasinfo","deletetuberculosisModify","closeId","handleQuery","resetQuery","handlerPageChange","pageNum","handlerPageSizeChange","pageSize"],"sourceRoot":"src/views/patientManagement/closurePatientManagement","sources":["index.vue"],"sourcesContent":["<template>\r\n  <div class=\"box\">\r\n    <div class=\"room\">\r\n      <el-form :model=\"form\" ref=\"form\" size=\"small\" :inline=\"true\" v-show=\"showSearch\" label-width=\"80px\" class=\"form_row\">\r\n        <el-row>\r\n          <el-col :span=\"6\">\r\n            <el-form-item label=\"行政区划:\" prop=\"RegionCode\">\r\n              <my-cascader v-model=\"form.RegionCode\" :props=\"props\" clearable></my-cascader>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"6\">\r\n            <el-form-item label=\"姓名:\" prop=\"Name\">\r\n              <el-input v-model=\"form.Name\" placeholder=\"请输入姓名\" clearable @keyup.enter.native=\"handleQuery\" />\r\n            </el-form-item>\r\n          </el-col>\r\n\r\n          <el-col :span=\"6\">\r\n            <el-form-item label=\"证件号:\" prop=\"CardNo\">\r\n              <el-input v-model=\"form.CardNo\" placeholder=\"请输入证件号\" clearable />\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"6\">\r\n            <el-form-item label=\"结案日期:\" prop=\"SubmitTime\">\r\n              <el-date-picker v-model=\"form.SubmitTime\" type=\"daterange\" align=\"center\" value-format=\"yyyy-MM-dd\"></el-date-picker>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span=\"6\">\r\n            <el-form-item label=\"管理机构:\" prop=\"AdminOrganization\">\r\n              <el-cascader\r\n                style=\"width: 100%\"\r\n                :options=\"tenantData\"\r\n                :props=\"propslist\"\r\n                v-model=\"form.AdminOrganization\"\r\n                clearable\r\n              ></el-cascader>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"2\">\r\n            <el-form-item label=\"仅本机构\" prop=\"SelectOnly\">\r\n              <el-checkbox v-model=\"form.SelectOnly\"></el-checkbox>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"4\">\r\n            <el-form-item>\r\n              <div class=\"searchbox\">\r\n                <div class=\"searchBtn\" @click=\"handleQuery\">\r\n                  <img class=\"Btnicon\" src=\"@/assets/images/searchicon2.png\" alt=\"\" />\r\n                  查询\r\n                </div>\r\n                <div class=\"ResetBtn\" style=\"margin-left: 10px\" @click=\"resetQuery\">\r\n                  <img class=\"Btnicon\" src=\"@/assets/images/reseticon.svg\" alt=\"\" />\r\n                  重置\r\n                </div>\r\n              </div>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row></el-row>\r\n      </el-form>\r\n      <!-- 表格区域 -->\r\n      <vxe-table align=\"center\" size=\"small\" :height=\"tableHeight\" stripe :data=\"tableData\" show-overflow class=\"mytable-scrollbar\">\r\n        <vxe-column field=\"Name\" title=\"姓名\" width=\"100\">\r\n\t\t\t\t\t<template slot-scope=\"{ row }\">\r\n\t\t\t\t\t\t<span>\r\n\t\t\t\t\t\t\t{{ row.Name | hideMiddle}}\r\n\t\t\t\t\t\t</span>\r\n\t\t\t\t\t</template>\r\n\t\t</vxe-column>\r\n        <vxe-column field=\"SexName\" title=\"性别\" width=\"80\"></vxe-column>\r\n        <vxe-column field=\"CardNo\" title=\"证件号\" width=\"150\">\r\n\t\t\t<template slot-scope=\"{ row }\">\r\n\t\t\t\t<span>\r\n\t\t\t\t\t{{ row.CardNo | hideMiddlelong}}\r\n\t\t\t\t</span>\r\n\t\t\t</template>\r\n\t\t</vxe-column>\r\n        <vxe-column field=\"Phone\" title=\"电话号码\" width=\"100\">\r\n\t\t\t<template slot-scope=\"{ row }\">\r\n\t\t\t\t<span>\r\n\t\t\t\t\t{{ row.Phone | hideMiddlelong}}\r\n\t\t\t\t</span>\r\n\t\t\t</template>\r\n\t\t</vxe-column>\r\n        <vxe-column field=\"Address\" title=\"居住地址\" min-width=\"200\">\r\n\t\t\t<template slot-scope=\"{ row }\">\r\n\t\t\t\t<span>\r\n\t\t\t\t\t{{ row.Address | hideMiddlelong}}\r\n\t\t\t\t</span>\r\n\t\t\t</template>\r\n\t\t</vxe-column>\r\n        <vxe-column field=\"AdminOrganizationName\" title=\"建档机构\" width=\"200\"></vxe-column>\r\n        <vxe-column field=\"PatientTypeName\" title=\"患者类型\" width=\"100\"></vxe-column>\r\n        <vxe-column field=\"EvaluationDate\" title=\"评估日期\">\r\n          <template #default=\"{ row }\">\r\n            <div>{{ row.EvaluationDate.slice(0, 10) }}</div>\r\n          </template>\r\n        </vxe-column>\r\n        <vxe-column title=\"操作\" width=\"200\" fixed=\"right\">\r\n          <template #default=\"{ row }\">\r\n            <div class=\"flex_center\">\r\n              <el-link type=\"primary\" @click=\"handlerEdit('details', row)\">查看详情</el-link>\r\n              <div class=\"Divider\"></div>\r\n              <el-link type=\"primary\" @click=\"handlerEdit('edit', row)\">编辑</el-link>\r\n              <div class=\"Divider\"></div>\r\n              <el-link type=\"danger\" @click=\"handlerEdit('recurrence', row)\">复发</el-link>\r\n              <div class=\"Divider\"></div>\r\n              <el-link type=\"danger\" @click=\"handlerEdit('delete', row)\">删除</el-link>\r\n            </div>\r\n          </template>\r\n        </vxe-column>\r\n      </vxe-table>\r\n      <!-- </div> -->\r\n      <!-- 分页器 -->\r\n      <div class=\"app-pagination\">\r\n        <el-pagination\r\n          ref=\"elPage\"\r\n          :layout=\"paginationLayout\"\r\n          :total=\"total\"\r\n          :current-page=\"paParams.PageNumber\"\r\n          :page-size=\"paParams.PageSize\"\r\n          @current-change=\"handlerPageChange\"\r\n          @size-change=\"handlerPageSizeChange\"\r\n        ></el-pagination>\r\n      </div>\r\n\r\n      <Edit ref=\"edit\"></Edit>\r\n      <CaseEdit ref=\"caseEdit\" @sumite=\"funtoapplysettlement\"></CaseEdit>\r\n      <RecurrenceEdit ref=\"recurrenceEdit\" @recurrenceEdit=\"recurrenceEdit\"></RecurrenceEdit>\r\n      <NewEdit ref=\"newedit\" @sumite=\"funtoapplysettlement\"></NewEdit>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n// 弹窗子组件\r\nimport Edit from './components/Edit.vue';\r\nimport RecurrenceEdit from './components/recurrenceEdit.vue';\r\nimport CaseEdit from '../patientManagement/components/CaseEdit.vue';\r\nimport NewEdit from '../patientManagement/components/newEdit.vue'\r\n// 级联选择器\r\nimport MyCascader from '@/components/MyCascader';\r\n// 结案\r\nimport {\r\n  toapplysettlement,\r\n  deletetuberculosisdiagnosiss\r\n} from '@/api/PulmonaryClose.js';\r\nimport {\r\n  getOrganizationList,\r\n  getorgcascader,\r\n  getuserDrop,\r\n\r\n\r\n} from '@/api/organizationManagement.js'\r\nexport default {\r\n  name: 'PatientRecords',\r\n  components: {\r\n    Edit,\r\n    RecurrenceEdit,\r\n    MyCascader,\r\n    CaseEdit,\r\n    NewEdit\r\n  },\r\n  data () {\r\n    return {\r\n      props: {\r\n        emitPath: false,\r\n        label: 'RegionName',\r\n        children: 'Children',\r\n        value: 'RegionCode',\r\n        multiple: false,\r\n        checkStrictly: true,\r\n        lazy: true,\r\n      },\r\n      propslist: {\r\n        value: \"OrganizationId\",\r\n        label: \"OrganizationName\",\r\n        children: \"Children\",\r\n        checkStrictly: true,\r\n        emitPath: false,\r\n        lazy: true,\r\n        lazyLoad: this.lazyLoad\r\n      },\r\n      // 表单绑定数据\r\n      form: {\r\n        SelectOnly: false,\r\n        AdminOrganization: this.$utilsFun.getUserInfo().authOrgId,\r\n        IsClosed: true,\r\n      },\r\n      // closeId:null,\r\n      radio1: '全部',\r\n      tenantData: [{\r\n        OrganizationId: this.$utilsFun.getUserInfo().authOrgId,\r\n        OrganizationName: this.$utilsFun.getUserInfo().authOrgName,\r\n        Children: []\r\n      }], //机构数组\r\n      showSearch: true,\r\n      total: 0,\r\n      paParams: {\r\n        PageNumber: 1,\r\n        PageSize: 10,\r\n      },\r\n      tableData: [],\r\n      tableHeight: '300', //table高度\r\n\r\n    };\r\n  },\r\n\r\n  async created () {\r\n    await this.funtoapplysettlement();\r\n    // await this.getorganizationlist()\r\n  },\r\n  mounted () {\r\n    this.initTableHeight();\r\n  },\r\n  methods: {\r\n    // 获取列表\r\n    async funtoapplysettlement () {\r\n      const res = await toapplysettlement({\r\n        ...this.paParams,\r\n        ...this.form,\r\n      });\r\n\r\n      let {\r\n        StatusCode,\r\n        Message,\r\n        Response\r\n      } = res;\r\n      if (!Response || StatusCode != 200) {\r\n        return this.$baseMessage(Message || '数据获取失败', 'error');\r\n      }\r\n      this.tableData = Response.items;\r\n      this.total = Response.TotalCount;\r\n    },\r\n    //获取机构列表\r\n    async getorganizationlist () {\r\n      this.form.AdminOrganization = this.$utilsFun.getUserInfo().authOrgId;\r\n      const res = await getorgcascader(this.form.AdminOrganization);\r\n      this.tenantData = res.Response;\r\n    },\r\n    async lazyLoad (node, resolve) {\r\n      let res = await getOrganizationList({\r\n        Org_OrganizationId: node.value,\r\n        PageNumber: 1,\r\n        PageSize: 9999,\r\n      })\r\n      let data = []\r\n      if (node.level > 0) {\r\n        data = res.Response.items\r\n      }\r\n      resolve(data);\r\n    },\r\n    //计算table的高度\r\n    initTableHeight () {\r\n      this.$nextTick(() => {\r\n        let availHeight = window.screen.availHeight; //页面可视化高度\r\n        var elPageClientHeight = this.$refs.elPage.$el.clientHeight; // 分页器高度\r\n        this.tableHeight = availHeight - elPageClientHeight - 400;\r\n      });\r\n    },\r\n    // 新增或修改按钮 弹窗显示\r\n    handlerEdit (flag, data) {\r\n      if (flag == 'details') {\r\n        this.$refs.edit.show(flag, data.CloseId);\r\n        // 复发？？？\r\n      } else if (flag == 'recurrence') {\r\n        this.$refs.newedit.show(flag, data)\r\n      } else if (flag == 'edit') {\r\n        this.$refs.caseEdit.show(flag, data)\r\n      } else if (flag == 'delete') {\r\n        // 删除\r\n        this.$baseConfirm(\"确定要删除所选项吗？\", '删除', async () => {\r\n          // id\r\n          let CloseId = data.CloseId\r\n          const res = await deletetuberculosisdiagnosiss(CloseId)\r\n          let {\r\n            StatusCode,\r\n            Message,\r\n            Response\r\n          } = res;\r\n          this.$baseMessage('删除成功', \"success\")\r\n          this.funtoapplysettlement()\r\n        },null,\"error\",)\r\n      } else if (flag == 'Modify') {\r\n        // 修改\r\n        this.$refs.recurrenceEdit.show(flag, data.CloseId);\r\n      }\r\n    },\r\n    //结案时间选择\r\n    changeSubmitTime (value) {\r\n      this.form.SubmitTime = value;\r\n    },\r\n\r\n    async recurrenceEdit (flag, form, hasinfo) {\r\n      let CloseId = form.CloseId;\r\n\r\n      const res = await deletetuberculosisModify({\r\n        closeId: CloseId,\r\n      },\r\n        hasinfo\r\n      );\r\n      let {\r\n        StatusCode,\r\n        Message\r\n      } = res;\r\n      if (StatusCode != 200) {\r\n        return this.$baseMessage(Message || '修改失败', 'error');\r\n      }\r\n      this.$baseMessage('修改成功', 'success');\r\n    },\r\n\r\n    // 查询搜索\r\n    handleQuery () {\r\n      this.funtoapplysettlement();\r\n    },\r\n    async resetQuery () {\r\n      this.form.Name = '';\r\n      this.form.CardNo = '';\r\n      this.form.IsTakeMedicine = '';\r\n      this.form.Sex = '';\r\n      this.form.SelectOnly = false;\r\n      this.form.UserAcc = '';\r\n      this.form.SubmitTime = '';\r\n      this.form.IsClosed = true;\r\n      this.form.RegionCode = '';\r\n      this.form.AdminOrganization = this.$utilsFun.getUserInfo().authOrgId;\r\n      const res = await toapplysettlement({\r\n        ...this.paParams,\r\n        ...this.form,\r\n      });\r\n      this.tableData = res.Response.items;\r\n      this.total = res.Response.TotalCount\r\n      this.$baseMessage('已重置', \"success\")\r\n    },\r\n\r\n    // 分页当前页变化回调\r\n    handlerPageChange (pageNum) {\r\n      this.paParams.PageNumber = pageNum;\r\n      this.funtoapplysettlement();\r\n    },\r\n    // 分页每页条数变化回调\r\n    handlerPageSizeChange (pageSize) {\r\n      this.paParams.PageSize = pageSize;\r\n      this.funtoapplysettlement();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}